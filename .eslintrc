{
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
  },
  "env": {
      "browser": true,
      "node": true,
      "es6": true
  },
  "extends": [
      "eslint:recommended"
  ],
  "plugins": [
      "import"
  ],
  "rules": {
      "no-console": 0
  }
}